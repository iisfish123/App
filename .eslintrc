{
    "env": {
       "es6": true,
       "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
    },
    "parser": "babel-eslint",
    "rules": {
        "no-unused-vars": 0,
        "quotes": ["error", "single"],
//        "comma-spacing": [2, { "before": false, "after": true }],
//        "arrow-spacing": "error",
        "no-duplicate-imports": "error",
        "no-var": "error",
        "eqeqeq": ["error", "always"],
    }
}
